{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "outDir": "./.dist",
    "paths": {
      "src/*": ["./src/*"]
    }
  },
  "include": ["api/index.js", "src/**/*.ts"]
}
