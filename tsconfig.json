{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "outDir": "./public",
    "moduleResolution": "node",
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "esModuleInterop": true,
    "noEmit": true,
    "paths": {
      "src/*": ["./src/*"]
    }
  }
}
